  "bin": {
    "ajs": "index.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "figlet": "^1.2.1",
    "ora": "^3.0.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
