    "tsc-watch": "^1.0.30",
    "typescript": "^3.1.5"
  },
  "scripts": {
    "watch": "./node_modules/.bin/tsc-watch",
    "build": "./node_modules/.bin/tsc && yarn build:contents && yarn build:readme && yarn build:package",
